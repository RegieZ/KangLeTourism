<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.regino.travel.dao.CategoryDao">

    <select id="findAll" resultType="com.regino.travel.domain.Category">
        SELECT * FROM tab_category ORDER BY cid
    </select>

    <select id="findByCid" resultType="com.regino.travel.domain.Category">
        SELECT * FROM tab_category where cid = #{cid}
    </select>

</mapper>